{
  "name": "clinical-trials",
  "version": "0.1.0",
  "description": "Test project: an app to apply for clinical trials",
  "author": "Volodymyr Galkin <vladgalkin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gulp"
  },
  "devDependencies": {
    "babel-loader": "^6.2.9",
    "babel-preset-latest": "^6.16.0",
    "del": "^2.2.2",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "webpack-stream": "^3.2.0"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "import"
    ],
    "rules": {
      "comma-dangle": [
